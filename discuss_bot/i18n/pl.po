#, python-format
msgid "help"
msgstr "pomoc"

#. module: discuss_bot
#: code:addons/discuss_bot/models/discuss_bot.py:0
#, python-format
msgid "i love you"
msgstr "kocham Cię"

#. module: discuss_bot
#: code:addons/discuss_bot/models/discuss_bot.py:0
#, python-format
msgid "love"
msgstr "miłość"

